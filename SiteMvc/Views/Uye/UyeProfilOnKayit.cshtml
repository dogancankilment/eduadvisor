@model List<dll.Models.on_kayit_basvuru>
<div class="tab-pane" id="tab_okul_kayit" style="margin-left: 36px;">
    @if (Model.Count == 0)
    {
        <div class="alert alert-success">
            <i class="icon-gift"></i>
            <strong>
                @Resources.Varsayilan.i_eduonkayityok<br />
                @Resources.Varsayilan.i_eduonkayiticerik @Resources.Varsayilan.i_edukayiticerikdevam<br>
                @Resources.Varsayilan.i_ustaramadanara
            </strong>
        </div>
    }
    else
    {
        <div id="onkayitsayfala" class="col-xs-12 nopadding">
            @for (int i = 0; i < Model.Count; i++)
            {
                <section class="row col-xs-12" style="padding:0;margin:0">
                    <div class="col-xs-6">
                        <div class="form-group">
                            <label style="color: #a02f89;">@Resources.Varsayilan.i_onkayitdurumu:</label>
                            <label class="marginleft10">
                                <span class="label font12 @(Model[i].durumu == 0 ? "label-warning": Model[i].durumu==1 ? "label-success" : "label-danger")">
                                    @(Model[i].durumu == 0 ? Resources.Varsayilan.b_beklemede : Model[i].durumu == 1 ? Resources.Varsayilan.b_onaylandi : Resources.Varsayilan.silindi)
                                </span>
                            </label>
                        </div>
                    </div>
                    <div class="col-xs-6">
                        <div class="form-group">
                            <label style="color: #a02f89;">@Resources.Varsayilan.b_basvkodu :</label><label class="marginleft10 label label-info font12">@Model[i].on_kayitkodu</label>
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="form-group">
                            <label style="color: #357169; margin-bottom: 20px">@Resources.Varsayilan.b_kisiselbilg</label>
                            <hr style="margin-top: 0px; border-top: 1px solid #238275;">
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_adinizsoyadiniz:</label><br>
                                <label>@(Model[i].uyeler.adi + " " + Model[i].uyeler.soyadi)</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_cinsiyet:</label><br>
                                <label>@(Model[i].cinsiyet == 0 ? Resources.Varsayilan.b_bayan : Model[i].cinsiyet == 1 ? Resources.Varsayilan.b_bay : Resources.Varsayilan.b_belirtmekistemiyorum)</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_milliyeti:</label><br>
                                <label>@Model[i].ulke1.adi</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_dogumtarihi:</label><br>
                                <label>@Model[i].dog_tar</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_dogulkesehir:</label><br>
                                <label>@((HttpContext.Current.Session["Dil"].ToString() == "tr-TR" ? Model[i].il.ulke.adi : Model[i].il.ulke.adi_ing) + " - " + Model[i].il.adi) </label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_yasulkesehir:</label><br>
                                <label>@((HttpContext.Current.Session["Dil"].ToString() == "tr-TR" ? Model[i].il1.ulke.adi : Model[i].il1.ulke.adi_ing) + " - " + Model[i].il1.adi)</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_passno:</label><br>
                                <label>@Model[i].pass_no</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_passtarih:</label><br>
                                <label>@Model[i].pass_tarih</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="form-group">
                            <label style="color: #357169; margin-bottom: 20px">@Resources.Varsayilan.b_basvbilg</label>
                            <hr style="margin-top: 0px; border-top: 1px solid #238275;">
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_basvokul:</label><br>
                                <label>@(Model[i].okullar.okul_grup_iliski.FirstOrDefault().okul_gruplari.adi + " - " + Model[i].okullar.adi)</label>
                            </div>
                        </div>
                        @if (Model[i].okullar.egitim_turleri.program_var)
                        {
                            <div class="col-xs-4">
                                <div class="form-group">
                                    <label style="color: #a02f89;">@Resources.Varsayilan.b_basprogturu:</label><br>
                                    <label>@Model[i].onkayit_egitim_turleri.adi</label>
                                </div>
                            </div>
                            <div class="col-xs-4">
                                <div class="form-group">
                                    <label style="color: #a02f89;">@Resources.Varsayilan.b_basvprogadi:</label><br>
                                    <label>@Model[i].program_havuzu.adi</label>
                                </div>
                            </div>
                        }
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;"> @Resources.Varsayilan.b_basvtarih</label><br>
                                <label>@(Model[i].basvuru_tarihi.ToString("dd/MM/yyyy HH:mm"))</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_basltarih</label><br>
                                <label>@Model[i].baslayacagi_tarih</label>
                            </div>
                        </div>
                    </div>

                    @if (Model[i].okullar.egitim_turleri.kurs_hafta_alinacak || Model[i].okullar.egitim_turleri.dil_bilgisi_alinacak)
                    {
                        <div class="col-xs-12">
                            <div class="form-group">
                                <label style="color: #357169; margin-bottom: 20px">@Resources.Varsayilan.b_dilbilg</label>
                                <hr style="margin-top: 0px; border-top: 1px solid #238275;">
                            </div>
                            @if (Model[i].okullar.egitim_turleri.kurs_hafta_alinacak)
                            {
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label style="color: #a02f89;">@Resources.Varsayilan.b_kurshafta:</label><br>
                                        <label>@Model[i].kurs_hafta </label>
                                    </div>
                                </div>
                            }
                            @if (Model[i].okullar.egitim_turleri.dil_bilgisi_alinacak)
                            {
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label style="color: #a02f89;">@Resources.Varsayilan.b_dilsvy:</label><br>
                                        <label>@Model[i].dil_seviye</label>
                                    </div>
                                </div>
                            }
                        </div>
                    }
                    <div class="col-xs-12">
                        <div class="form-group">
                            <label style="color: #357169; margin-bottom: 20px">@Resources.Varsayilan.b_iltbilg</label>
                            <hr style="margin-top: 0px; border-top: 1px solid #238275;">
                        </div>

                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.bslk_email:</label><br>
                                <label>@Model[i].email</label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_ceptel :</label><br>
                                <label>@Model[i].tel_cep </label>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_evtel:</label><br>
                                <label>@Model[i].tel_ev </label>
                            </div>
                        </div>
                        <div class="col-xs-12">
                            <div class="form-group">
                                <label style="color: #a02f89;">@Resources.Varsayilan.b_adres :</label><br>
                                <label>@Model[i].adres </label>
                            </div>
                        </div>
                    </div>
                </section>
            }
        </div>
    }
</div>