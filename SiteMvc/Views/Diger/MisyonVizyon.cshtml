@{ 
    ViewBag.Title = Resources.Varsayilan.misyonumuz;
}

<section id="content" style="margin-bottom: 0px;">
    <div class="content-wrap">
        <div class="container clearfix">
            <div class="col_full">
                <div class="slide flex-active-slide" style="width: 100%; float: left; margin-right: -100%; position: relative; opacity: 1; display: block; z-index: 2;">
                    <img src="~/Content/img/hakkimizda.png" alt="EduAdvisor @Resources.Varsayilan.misyonumuz">
                </div>
            </div>
        </div>
        <div class="container clearfix">
            <blockquote class="topmargin bottommargin">
                <p>@Html.Raw(ViewBag.SayfaIcerik.Replace("\\n", "<br/>"))</p>
            </blockquote>
        </div>
    </div>
</section>
