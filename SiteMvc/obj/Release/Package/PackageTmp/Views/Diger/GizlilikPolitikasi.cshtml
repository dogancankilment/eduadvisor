@{
    ViewBag.Title = Resources.Varsayilan.gizlilik_politikasi;
}

<section id="page-title">
    <div class="container clearfix">
        <span id="baslik" style="color:#a02f89">@Resources.Varsayilan.gizlilik_politikasi</span>
    </div>
</section>

<section id="map-overlay">
    <div class="container clearfix">
        <div class="container clearfix">
            <blockquote class="topmargin bottommargin">
                <p>@Html.Raw(ViewBag.SayfaIcerik.Replace("\\n", "<br/>").Replace("\\r", ""))</p>
            </blockquote>
        </div>
    </div>
</section>




