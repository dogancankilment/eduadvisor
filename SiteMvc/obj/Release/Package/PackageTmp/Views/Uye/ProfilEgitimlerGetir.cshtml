@model List<dll.Models.uye_okullari>

@if (Model.Count == 0)
            {
    <div class="alert alert-danger nobottommargin">
        <i class="icon-gift"></i><strong>@Resources.Varsayilan.u_okulkaydiyok..</strong>
    </div>
}
else
{
    <table class="grd_egitim" cellspacing="0" rules="all" border="1" style="border-collapse:collapse;">
        <thead>
            <tr>
                <th class="templatepadding" scope="col">@Resources.Varsayilan.b_egitimturu</th>
                <th class="templatepadding" scope="col">@Resources.Varsayilan.b_okuladi</th>
                <th class="templatepadding" scope="col">@Resources.Varsayilan.b_programadi</th>
                <th class="templatepadding" scope="col">@Resources.Varsayilan.b_egitimdonemi</th>
                <th class="templatepadding" scope="col">&nbsp;</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count; i++)
                {
                <tr>
                    <td class="itempadding">
                        <label class="egitim_tur">@Model[i].okullar.egitim_turleri.adi</label>
                    </td>
                    <td class="itempadding text-center">
                        <label>@Model[i].okullar.okul_grup_iliski.FirstOrDefault().okul_gruplari.adi</label>
                        <br />
                        <small>@Model[i].okullar.adi</small>
                    </td>
                    <td class="itempadding">
                        <label>@(HttpContext.Current.Session["Dil"].ToString() == "tr-TR" ? (Model[i].program_havuzu.adi + "-" + Model[i].onkayit_egitim_turleri.adi) : (Model[i].program_havuzu.adi_ingilizce + "-" + Model[i].onkayit_egitim_turleri.adi_ingilizce))</label>
                    </td>
                    <td class="itempadding">
                        <label class="font12">
                            @(Model[i].baslangic + " - " + (Model[i].bitis == "-1" ? Resources.Varsayilan.b_haladevam : Model[i].bitis))
                        </label>
                    </td>
                    <td class="itempadding">

                        <a href="@Url.RouteUrl("OkulDetay", new { GrupSeo = Model[i].okullar.okul_grup_iliski.FirstOrDefault().okul_gruplari.seo_url, OkulSeo = Model[i].okullar.seo_url })" tar target="_blank"
                           class="button button-small button-circle button-green">@Resources.Varsayilan.b_yorumyap</a>
                        <a onclick="ProfilEgitimSil(event, @Model[i].id)" data-s="@Resources.Varsayilan.uye_profil_okul_sil_onay" class="button button-small button-circle button-red">@Resources.Varsayilan.b_sil</a>
                    </td>
                </tr>
            }


        </tbody>
    </table>
}